{
    "container": {
        "docker": {
            "image": "gurulearningxyz/cd-demo-app:latest",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 8081,
                    "protocol": "tcp"
                }
            ]
        },
        "type": "DOCKER"
    },
    "cpus": 0.1,
    "id": "cd-demo-app",
    "instances": 1,
    "labels": {
        "HAPROXY_0_VHOST": "hellovalassis.plumlabs.us",
        "HAPROXY_GROUP": "dev",
    },
    "mem": 128,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0
    }
}
